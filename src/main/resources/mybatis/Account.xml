<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.AccountDao">
    <insert id="saveAccount" parameterType="com.entity.Account">
        insert into account(
        id,
        name,
        money
        )
        values(
        #{id},
        #{name},
        #{money}
        )
    </insert>
</mapper>